%%%%%%%%%%%%%%%%%%%%%% PERTURBATION, NOMINAL, DISTURBANCE %%%%%%%%%%%%%%%%%%
\newcommand{\perturb}[1]{\ensuremath{\widetilde{#1}}}
\newcommand{\perturbdot}[1]{\ensuremath{\dot{\widetilde{#1}}}}
\newcommand{\perturbddot}[1]{\ensuremath{\ddot{\widetilde{#1}}}}
\newcommand{\perturbdddot}[1]{\ensuremath{\dddot{\widetilde{#1}}}}
   % test: \perturb{x}       % produces a widetilde over x
   % test: \perturbdot{x}    % produces a widetilde over x and a dot over that
   % test: \perturbddot{x}   % produces a widetilde over x and a double dot over that
   % test: \perturbdddot{x}  % produces a widetilde over x and a double dot over that and a dot over that

\newcommand{\Nominal}{\ensuremath{\mathrm{nom}}}
\newcommand{\Desired}{\ensuremath{\mathrm{des}}}
\newcommand{\Specified}{\ensuremath{\mathrm{spec}}}
\newcommand{\Disturbance}{\ensuremath{\mathrm{disturbance}}}
   % test: \Nominal         % useful for subscript on Taylor series (rather than listing the entire nominal solution)
   % test: \Desired
   % test: \Specified
   % test: \Disturbance

\newcommand{\nominal}[1]{\ensuremath{{\/#1}_\Nominal}}
\newcommand{\nominaldot}[1]{\ensuremath{{\/\dot{#1}}_\Nominal}}
\newcommand{\nominalddot}[1]{\ensuremath{{\/\ddot{#1}}_\Nominal}}
   % test: \nominal{x}      % produces a subscript ``nominal'' to the lower right of x
   % test: \nominaldot{x}   % produces a subscript ``nominal'' to the lower right of \dot{x}
   % test: \nominalddot{x}  % produces a subscript ``nominal'' to the lower right of \ddot{x}

\newcommand{\desired}[1]{\ensuremath{{\/#1}_\Desired}}
