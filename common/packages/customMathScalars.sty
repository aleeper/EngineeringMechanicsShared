\newcommand{\specified}[1]{\ensuremath{{\/#1}_\Specified}}
\newcommand{\specifiedDot}[1]{\ensuremath{{\/\dot{#1}}_\Specified}}
\newcommand{\specifiedDdot}[1]{\ensuremath{{\/\ddot{#1}}_\Specified}}

\newcommand{\disturbance}[1]{\ensuremath{{\/#1}_\Disturbance}}
\newcommand{\disturbancedot}[1]{\ensuremath{{\/\dot{#1}}_\Disturbance}}
